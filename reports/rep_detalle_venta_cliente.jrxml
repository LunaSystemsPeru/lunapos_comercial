	<parameter name="fecha" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
  cli.nombre,
  DATE_FORMAT(ven.fecha, "%m %e %Y") AS fecha,
  pro.descripcion,
  pv.precio,
  pv.cantidad,
  (pv.precio * pv.cantidad) AS total 
FROM
  productos AS pro 
  INNER JOIN productos_ventas AS pv 
    ON pro.id_producto = pv.id_producto 
  INNER JOIN ventas AS ven 
    ON pv.id_ventas = ven.id_ventas 
  INNER JOIN clientes AS cli 
    ON ven.id_cliente = cli.id_cliente 
WHERE ven.fecha =  $P{fecha} 
  ORDER BY  cli.nombre  , pro.descripcion  ASC]]>
	</queryString>
